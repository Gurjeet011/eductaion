<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <RelativeLayout
        android:id="@+id/gr"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginStart="10dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/outline2">

        <TextView
            android:id="@+id/txt1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text="SSC GD Mock Test-01"
            android:textColor="@color/black"
            android:textSize="18sp" />



        <TextView
            android:id="@+id/txt3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt1"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text="Questions"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txt4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt3"
            android:layout_alignStart="@+id/txt3"
            android:layout_alignEnd="@+id/txt3"
            android:gravity="center_horizontal"
            android:text="40"
            android:textColor="@color/black"
            android:textSize="18sp" />


        <TextView
            android:id="@+id/txt5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/txt3"
            android:layout_below="@id/txt1"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text="Marks"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txt6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt5"
            android:layout_alignStart="@+id/txt5"
            android:layout_alignEnd="@+id/txt5"
            android:gravity="center_horizontal"
            android:text="80"
            android:textColor="@color/black"
            android:textSize="18sp" />
        <TextView
            android:id="@+id/txt7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_toRightOf="@id/txt5"
            android:layout_below="@id/txt1"
            android:layout_marginLeft="10dp"
            android:layout_marginTop="10dp"
            android:text="Minutes"
            android:textColor="@color/black"
            android:textSize="18sp" />

        <TextView
            android:id="@+id/txt8"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@id/txt7"
            android:layout_alignStart="@+id/txt7"
            android:layout_alignEnd="@+id/txt7"
            android:gravity="center_horizontal"
            android:text="30"
            android:textColor="@color/black"
            android:textSize="18sp" />
        
        
        <ImageView
            android:id="@+id/logoc"
            android:layout_width="100dp"
            android:layout_height="100dp"
            android:src="@drawable/ssc"
            android:layout_alignParentEnd="true"/>


        <View
            android:id="@+id/viewline"
            android:layout_width="match_parent"
            android:layout_height="1dp"
            android:background="@color/black"
            android:layout_marginHorizontal="10dp"
            android:layout_below="@id/txt8"
            android:layout_marginTop="10dp"/>

        <androidx.appcompat.widget.AppCompatButton
            android:id="@+id/attempt"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@id/viewline"
            android:layout_marginHorizontal="30dp"
            android:layout_marginVertical="10dp"
            android:background="@drawable/signup"
            android:text="Attempt"
            android:textColor="@color/black"
            android:textSize="18sp"
            android:textAllCaps="false">

        </androidx.appcompat.widget.AppCompatButton>


    </RelativeLayout>


</RelativeLayout>